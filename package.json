{
  "name": "ai-utils",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "svelte-kit sync",
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier --check . && eslint .",
